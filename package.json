    "coffee-script":          "~1.6.2",
    "mocha": "~1.9.0"
  },
  "repository": {
    "type":   "git",
    "url":    "https://github.com/TheDistractor/CCXMLEventEmitter"
  },
  "bugs": {
    "url":    "https://github.com/TheDistractor/CCXMLEventEmitter"
  },
