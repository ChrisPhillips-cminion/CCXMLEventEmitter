{ "name":           "ccxmleventemitter",
  "version":        "0.9.3beta",
  "description":    "CurrentCost Base Station XML NodeJS EventEmitter",
  "homepage":       "https://github.com/TheDistractor/CCXMLEventEmitter",
  "author":         "Andy Spencer <lightbulb@laughlinez.com>",
  "contributors": [
    "Andy Spencer <lightbulb@laughlinez.com>"
  ],
  "keywords": [
    "currentcost", "current cost", "xml", "eventemitter", "nodejs", "cc128", "envir"
  ],
  "main":           "lib/ccxmleventemitter",
  "scripts": {
